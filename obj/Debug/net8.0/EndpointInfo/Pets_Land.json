{
  "openapi": "3.0.1",
  "info": {
    "title": "Pets_Land",
    "version": "1.0"
  },
  "paths": {
    "/api/auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequestDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequestDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequestDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Products/add": {
      "post": {
        "tags": [
          "Products"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "Name": {
                    "type": "string"
                  },
                  "Price": {
                    "type": "number",
                    "format": "double"
                  },
                  "OldPrice": {
                    "type": "number",
                    "format": "double"
                  },
                  "Image": {
                    "type": "string",
                    "format": "binary"
                  },
                  "Category": {
                    "type": "string"
                  },
                  "Seller": {
                    "type": "string"
                  },
                  "Stock": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "Description": {
                    "type": "string"
                  },
                  "Ingredients": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              },
              "encoding": {
                "Name": {
                  "style": "form"
                },
                "Price": {
                  "style": "form"
                },
                "OldPrice": {
                  "style": "form"
                },
                "Image": {
                  "style": "form"
                },
                "Category": {
                  "style": "form"
                },
                "Seller": {
                  "style": "form"
                },
                "Stock": {
                  "style": "form"
                },
                "Description": {
                  "style": "form"
                },
                "Ingredients": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/User/SignUp": {
      "post": {
        "tags": [
          "User"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/UserDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/UserDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginRequestDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UserDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "phoneNo": {
            "type": "integer",
            "format": "int32"
          },
          "userName": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}